<% Song.all.each do |song| %>
  <div class="song">
    <h3><a href="/songs/<%= song.id %>"><%= song.title %></a> <small>(by: <%= song.author %>)</small></h3>
    <p>
      <a href="http://<%= song.url %> " target="_blank"><%= song.url %></a>
    </p>

    
  </div>
<% end %>

<a href="/songs/new">Post your new song!</a>